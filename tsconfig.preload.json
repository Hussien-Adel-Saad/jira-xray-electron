{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    "lib": ["ES2020"],
    "outDir": "./dist-electron",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "noEmit": false,
    "allowImportingTsExtensions": false
  },
  "include": [
    "src/main/preload.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
